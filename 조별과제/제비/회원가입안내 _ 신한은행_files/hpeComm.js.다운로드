// BODY 태그에 css추가
hpeComm.initBodyCss = function(cssArray, cssDesc)  {
	try {
		for(var key in cssArray) {
			var cssName = cssArray[key];
			
			// 없으면 추가
			var isHasClass = $("body").hasClass(cssName);
			if(!isHasClass) {
				$("body").addClass(cssName);
			}
		} // end of for
	}catch(e){}
	
	// TODO:결과확인 메세지
	//alert('TODO : css 초기화완료 TEST -->' + cssDesc);
};

